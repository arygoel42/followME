{
    "builds": [
      {
        "src": "/client/client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "/client/client/build"
        }
      },
      {
        "src": "backend/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/client/client/build/$1"
      }
    ]
  }